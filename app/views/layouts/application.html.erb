<!DOCTYPE html>
<html>
  <head>
    <title>MapMyHike</title>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{'AIzaSyC8WD1gp_Vbo6zJh2p_xQbbt07lF81XLss'}" %>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <script type="text/javascript">
      <% if logged_in %>
        window.currentUser = <%= render("api/users/user.json.jbuilder", user: current_user).html_safe %>
      <% end %>
    </script>
    <%= yield %>
  </body>
</html>
